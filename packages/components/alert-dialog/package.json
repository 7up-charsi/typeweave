{
  "name": "@webbo-ui/alert-dialog",
  "version": "1.0.9",
  "keywords": [
    "alert-dialog"
  ],
  "author": {
    "name": "Muhammad Zeeshan",
    "email": "7up.charsi@gmail.com"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/7up-charsi/webbo-ui.git",
    "directory": "packages/components/alert-dialog"
  },
  "bugs": {
    "url": "https://github.com/7up-charsi/webbo-ui/issues"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@webbo-ui/use-controllable-state": "workspace:*",
    "@webbo-ui/use-callback-ref": "workspace:*",
    "@webbo-ui/use-scroll-lock": "workspace:*",
    "@webbo-ui/focus-trap": "workspace:*",
    "@webbo-ui/overlay": "workspace:*",
    "@webbo-ui/slot": "workspace:*",
    "@webbo-ui/context": "workspace:*",
    "@webbo-ui/theme": "workspace:*"
  },
  "clean-package": {
    "extends": "../../../../../../clean-package.config.json"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  }
}
