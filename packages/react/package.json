    "react-dom": ">=18"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.0.8",
    "@ux-weaver/react-utils": "workspace:*",
    "@ux-weaver/theme": "workspace:*",
    "lodash.groupby": "^4.6.0",
    "lucide-react": "^0.376.0"
  },
  "devDependencies": {
    "tsconfig": "workspace:*",
    "@types/lodash.groupby": "^4.6.9",
    "clean-package": "^2.2.0"
  },
  "clean-package": {
    "extends": "../../../../../../clean-package.config.json"
  }
